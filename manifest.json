{
  "manifest_version": 3,
  "name": "copyPuppeteer",
  "version": "0.0.1",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "unlimitedStorage",
    "http://*/*",
    "https://*/*",
    "file:///*/*",
    "appNotifications",
    "background",
    "bookmarks",
    "clipboardRead",
    "clipboardWrite",
    "contentSettings",
    "contextMenus",
    "cookies",
    "debugger",
    "history",
    "idle",
    "management",
    "notifications",
    "pageCapture",
    "tabs",
    "topSites",
    "webNavigation",
    "webRequest",
    "webRequestBlocking"
  ],
  "description": "extension description",
  "background": {
    "page": "background.html",
    "matches": [
      "<all_urls>"
    ]
  },
  "content_scripts": [
  ]
} 